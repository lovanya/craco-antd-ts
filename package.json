{
  "name": "admin-antd-ts",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "craco start",
    "build": "craco build",
    "lint": "eslint --ext .js,.jsx,.ts,.tsx ./src",
    "lint:fix": "eslint --ext .js,.jsx,.ts,.tsx ./src --fix",
    "lint:style": "stylelint \"**/*.css\"",
    "lint:style:fix": "stylelint \"**/*.css\" --fix",
    "format": "prettier -c --write \"src/**/*\"",
    "lint-staged": "lint-staged",
    "test": "craco test"
